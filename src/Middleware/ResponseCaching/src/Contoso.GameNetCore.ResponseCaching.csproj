<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <Description>ASP.NET Core middleware for caching HTTP responses on the server.</Description>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <IsGameNetCoreApp>true</IsGameNetCoreApp>
    <NoWarn>$(NoWarn);CS1591</NoWarn>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <PackageTags>aspnetcore;cache;caching</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <Reference Include="Contoso.GameNetCore.ResponseCaching.Abstractions" />
    <Reference Include="Contoso.GameNetCore.Proto.Extensions" />
    <Reference Include="Contoso.GameNetCore.Proto" />
    <Reference Include="Microsoft.Extensions.Caching.Memory" />
    <Reference Include="Microsoft.Extensions.Logging.Abstractions" />
  </ItemGroup>

</Project>
